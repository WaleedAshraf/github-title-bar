{
  "name": "GitHub Title Bar",
  "version": "1.0.2",
  "description": "Static Title Bar for GitHub Issues.",
  "manifest_version": 2,
  "permissions": [
    "https://github.com/*"
  ],
  "icons": {
    "16": "github-16.png",
    "48": "github-48.png",
    "128": "github-128.png"
  },
  "browser_action": {
    "default_icon": "github-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/issues/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "contentScript.js"
      ],
      "css": [
        "style.css"
      ],
      "run_at": "document_end"
    }
  ]
}